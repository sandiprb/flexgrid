@import "_config.pcss";

*{
	box-sizing: border-box;
	&:after,
	&:before{
		box-sizing: border-box;
	}
}

.container-fluid{
	margin-left: auto;
	margin-right: auto;
	padding-left: $grid-gutter;
	padding-right: $grid-gutter;
}

.container{
	margin-left: auto;
	margin-right: auto;
	padding-left: $grid-gutter;
	padding-right: $grid-gutter;
}

@media (min-width: $screen-sm) {
	.container {
		width: 750px;
	}
}

@media (min-width: $screen-md) {
	.container {
		width: 970px;
	}
}

@media (min-width: $screen-lg) {
	.container {
		width: 1170px;
	}
}

/* row */
.fx-row{
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
	flex: 1 0 0;
	margin-left: -$grid-gutter;
	margin-right: -$grid-gutter;
}


