.fx-col-lg-5,
.fx-col-lg-4,
.fx-col-lg-3,
.fx-col-lg-2,
.fx-col-lg-1{
	padding-left: $grid-gutter;
	padding-right: $grid-gutter;
	min-height: 1px;
}

/* columns xs */
.fx-col-xs-12{
	flex-basis: 100%;
	max-width: 100%;
